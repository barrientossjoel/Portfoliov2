{
  "name": "portfoliov2",
  "description": "Barrientos Joel Portfolio",
  "keywords": [
    "portfolio",
    "astro",
    "porfolio-template",
    "astrojs",
    "astro-template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/barrientossjoel/Portfoliov2"
  },
  "license": "MIT",
  "author": "Barrientos Joel",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "prepare": "husky",
    "preview": "astro preview",
    "start": "astro dev",
    "deploy": "astro build && gh-pages -d dist"
  },
  "lint-staged": {
    "src/./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix .js,.mjs,.cjs ./",
      "prettier --cache --write \"./**/*.{js,mjs,cjs}\""
    ],
    "src/./**/*.ts": [
      "eslint --cache --fix .ts ./",
      "prettier --cache --write \"./**/*.ts\""
    ],
    "src/./src/**/*.astro": [
      "eslint --cache --fix .astro ./src/",
      "prettier --cache --write \"./src/**/*.astro\""
    ],
    "./src/**/*.css": "prettier --cache --write \"./src/**/*.css\"",
    "src/./**/*.json": "prettier --cache --write \"./**/*.json\"",
    "src/./**/*.yml": "prettier --cache --write \"./**/*.yml\""
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrolicious/i18n": "0.6.1",
    "@fontsource-variable/inter": "5.1.0",
    "@playform/compress": "0.1.6",
    "astro": "^4.16.18",
    "astro-compressor": "1.0.0",
    "astro-icon": "1.1.5",
    "astro-imagetools": "0.9.0",
    "astro-seo-schema": "^4.0.3",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.6",
    "chalk": "^4.1.2",
    "get-stream": "^9.0.1",
    "i18next": "^23.16.8",
    "minimatch": "^10.0.1",
    "module": "^1.2.5",
    "package": "^1.0.1",
    "sanitize.css": "13.0.0",
    "schema-dts": "1.1.2",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.0",
    "astro-eslint-parser": "1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.6"
  }
}
